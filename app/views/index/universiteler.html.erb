			<div class="row"
			style="height: 60%; padding-bottom: 20px; margin-bottom: 30px; margin-top: 20px;">

				<div class="row" style="padding-left: 20px; padding-right: 20px;">

					<div class="col-lg-3 hidden-xs hidden-sm hidden-md">

						<%= render "kullanicipaneli"   %>

						<div class="panel panel-danger">
							<div class="panel-heading">
								Üniversiteler
							</div>
							<div class="panel-body ">
								
								
								<%= form_tag '/universiteler' , :method => :get do %>

						<%= select_tag :universities,options_for_select(University.order(:name).collect { |a| [a.name, a.id] },params[:universities]),{:onchange => "this.form.submit()",style:"width:100%; "} %>

						<% end %>
								
								
							</div>
						</div>

						

						<div class="well">

							Buraya kafana göre bir şeyler ekle...
							<br>
							...
							<br>
							...
							<br>
							...
							<br>
							...
							<br>
							...
							<br>
							...
							<br>
							...
							<br>
							...
							<br>

						</div>

					</div>

					<%= render "kullanicipaneli2" %>

					<br class="visible-sm visible-xs visible-md">
					<br class="visible-sm visible-xs visible-md">

					<div class="col-lg-9 col-md-12 col-sm-12 col-xs-12"
					style="padding-left: 20px; padding-right: 20px">

						<div style="">

							<div class="panel panel-success">

								<div class="panel-heading">
									<h3 style="text-align: center;" class="panel-title"><%= @title %></h3>
								</div>

								<div class="panel-body">
									<% if @adverts!=[] %>
									<% @adverts.each do |advert|  %>

									<div class="col-lg-2 col-md-3 hidden-xs hidden-sm">

										<div class="thumbnail">
											<a href="<%= advert.href %>"> <% if advert.images == [] %>
											<%= image_tag Image.new.imagefile,:title => advert.name,class: "masterTooltip" %>
											<% else %>
											<%= image_tag advert.images[0].imagefile(:small),:title => advert.name,class: "masterTooltip" %>
											<% end %> </a>

										</div>

										<div class="caption">

											<% if advert.name.length <= 13 %>

											<p><%= advert.name %></p>
											

											<% else %>

											<p><%= advert.name[0..12] + "..." %></p>

											<% end %>

										</div>

									</div>
									
									
									
									
									<div class="visible-xs visible-sm  col-sm-4 col-xs-6">

										<div class="thumbnail">
											<a href="<%= advert.href %>"> <% if advert.images == [] %>
											<%= image_tag Image.new.imagefile,:title => advert.name,class: "masterTooltip" %>
											<% else %>
											<%= image_tag advert.images[0].imagefile(:small),:title => advert.name,class: "masterTooltip" %>
											<% end %> </a>

										</div>

										<div class="caption">

											<% if advert.name.length <= 10 %>

											<p><%= advert.name %></p>
											

											<% else %>

											<p><%= advert.name[0..9] + "..." %></p>

											<% end %>

										</div>

									</div>

									<% end %>

									<% else %>

									<span>Henüz hiç ilan bulunmamaktadır!!</span>

									<% end %>

								</div>

							</div>

						</div>

					</div>

				</div>

