                            </div>
                            <div class="panel-body">
                                <div id="morris-area-chart"></div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="/resources/plugins/adminpanel/js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/resources/plugins/adminpanel/js/bootstrap.min.js"></script>

