		<script type="text/javascript">
			$uyepaneljs(function() {
				var panels = $uyepaneljs('.user-infos');
				var panelsButton = $uyepaneljs('.dropdown-user');
				panels.hide();

				//Click dropdown
				panelsButton.click(function() {
					//get data-for attribute
					var dataFor = $uyepaneljs(this).attr('data-for');
					var idFor = $uyepaneljs(dataFor);

					//current button
					var currentButton = $(this);
					idFor.slideToggle(400, function() {
						//Completed slidetoggle
						if (idFor.is(':visible')) {
							currentButton.html('<i class="glyphicon glyphicon-chevron-up text-muted"></i>');
						} else {
							currentButton.html('<i class="glyphicon glyphicon-chevron-down text-muted"></i>');
						}
					})
				});

				$uyepaneljs('[data-toggle="tooltip"]').tooltip();

				$uyepaneljs('button').click(function(e) {
					e.preventDefault();
					alert("This is a demo.\n :-)");
				});
			});
		</script>

