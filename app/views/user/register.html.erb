						<script>
							function loginyonlendir() {

								window.location = "/girisyap";

							}

						</script>

						<!-- user name validation -->

						<script>
							function usernamevalidation() {

								
								var form = document.getElementById("kayitformu");

								var username = form.elements['user_username'].value;
								var usernameRegexp = new RegExp("^[a-zA-Z0-9._-]{4,15}$");

								var check = usernameRegexp.test(username);
								
								var result = false;

								if (username == "") {

									$(".usernameerror").hide();
									$(".usernamevalid").hide();
									$(".uctenkucukhata").hide();
									$(".karakterhatasi").hide();
									$(".usernameboskalamaz").show();

								} else if (username.length < 4) {

									$(".usernameerror").hide();
									$(".usernamevalid").hide();
									$(".karakterhatasi").hide();
									$(".usernameboskalamaz").hide();
									$(".uctenkucukhata").show();

								} else if (!check) {

									$(".usernameerror").hide();
									$(".usernamevalid").hide();
									$(".uctenkucukhata").hide();
									$(".usernameboskalamaz").hide();
									$(".karakterhatasi").show();

								} else {

									$.ajax({
										url : 'checkusername',
										data : {
											authenticity_token : window._token,
											username : username
										},
										error : function() {
											alert('error');
										},
										dataType : 'json',
										success : function(data) {

											$(".usernameerror").hide();
											$(".usernamevalid").hide();
											$(".uctenkucukhata").hide();
											$(".karakterhatasi").hide();
											$(".usernameboskalamaz").hide();

											if (!data.check) {

												$(".usernameerror").show();

											} else {

												$(".usernamevalid").show();
												result = true;

											}

										},
										type : 'POST',
										async: false
									});

								}
								
								return result;

							}

							function namevalidator() {

								var form = document.getElementById("kayitformu");

								var name = form.elements['user_name'].value;

								var nameRegexp = new RegExp("^[a-zA-Z\u00c7\u00e7\u011e\u011f\u0130\u0131\u00d6\u00f6\u015e\u015f\u00dc\u00fc]{1,20}$");

								var check = nameRegexp.test(name);
								
								var result = false;

								if (name == "") {

									$(".namekarakterhatasi").hide();
									$(".nameboskalamaz").show();

								} else if (!check) {

									$(".nameboskalamaz").hide();

									$(".namekarakterhatasi").show();

								} else {

									$(".namekarakterhatasi").hide();
									$(".nameboskalamaz").hide();
									result = true;

								}
								
								return result;

							}

							function surnamevalidator() {

								var form = document.getElementById("kayitformu");

								var surname = form.elements['user_surname'].value;

								var surnameRegexp = new RegExp("^([a-zA-Z\u00c7\u00e7\u011e\u011f\u0130\u0131\u00d6\u00f6\u015e\u015f\u00dc\u00fc]*([ '-][a-zA-Z\u00c7\u00e7\u011e\u011f\u0130\u0131\u00d6\u00f6\u015e\u015f\u00dc\u00fc])*){1,20}$");

								var check = surnameRegexp.test(surname);
								
								var result = false;

								if (surname == "") {

									$(".surnamekarakterhatasi").hide();
									$(".surnameboskalamaz").show();

								} else if (!check) {

									$(".surnameboskalamaz").hide();

									$(".surnamekarakterhatasi").show();

								} else {

									$(".surnamekarakterhatasi").hide();
									$(".surnameboskalamaz").hide();
									result = true;

								}
								
								return result;

							}

							function passwordcheck() {

								var form = document.getElementById("kayitformu");

								var password = form.elements['user_password'].value;

								var passRegexp = new RegExp("((?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{6,20})");

								var check = passRegexp.test(password);
								
								var result = false;

								if (password == "") {

									$(".passkarakterhatasi").hide();

									$(".altidankucukhata").hide();

									$(".passboskalamaz").show();

								} else if (password.length < 6) {

									$(".passkarakterhatasi").hide();
									$(".passboskalamaz").hide();

									$(".altidankucukhata").show();

								} else if (!check) {

									$(".passboskalamaz").hide();

									$(".altidankucukhata").hide();

									$(".passkarakterhatasi").show();

								} else {

									$(".passkarakterhatasi").hide();
									$(".passboskalamaz").hide();

									$(".altidankucukhata").hide();
									result = true;

								}

								return result && passwordrepeatcheck();

							}

							function passwordrepeatcheck() {

								var form = document.getElementById("kayitformu");

								var password = form.elements['user_password'].value;

								var password2 = form.elements['user_password_confirmation'].value;

								var check = password == password2;
								
								var result = false;

								if (!check) {

									$(".passkarakterhatasirepeat").show();

								} else {

									$(".passkarakterhatasirepeat").hide();
									result = true;

								}
								
								return result;

							}

							function emailcheck() {

								var form = document.getElementById("kayitformu");

								var email = form.elements['user_email'].value;

								var emailRegexp = new RegExp("^[_A-Za-z0-9-\\+]+(\\.[_A-Za-z0-9-]+)*@" + "[A-Za-z0-9-]+(\\.[A-Za-z0-9]+)*(\\.[A-Za-z]{2,})$");

								var check = emailRegexp.test(email);
								
								var result = false;

								if (email == "") {

									$(".emailerror").hide();
									$(".emailvalid").hide();
									$(".emailkarakterhatasi").hide();
									$(".emailboskalamaz").show();
									$(".uniemailerror").hide();

								} else if (!check) {

									$(".emailerror").hide();
									$(".emailvalid").hide();
									$(".emailboskalamaz").hide();
									$(".emailkarakterhatasi").show();
									$(".uniemailerror").hide();

								} else {

									$.ajax({
										url : 'checkemail',
										data : {
											authenticity_token : window._token,
											email : email
										},
										error : function() {
											alert('error');
										},
										dataType : 'json',
										success : function(data) {

											if (!data.check) {

												$(".emailboskalamaz").hide();
												$(".emailkarakterhatasi").hide();
												$(".emailvalid").hide();
												$(".emailerror").show();
												$(".uniemailerror").hide();

											}
											
											else if(!data.unicheck){
												
												$(".emailboskalamaz").hide();
												$(".emailkarakterhatasi").hide();
												$(".emailvalid").hide();
												$(".emailerror").hide();
												$(".uniemailerror").show();
												
											}
											
											 else {

												$(".emailboskalamaz").hide();
												$(".emailkarakterhatasi").hide();
												$(".emailerror").hide();
												$(".emailvalid").show();
												$(".uniemailerror").hide();
												
												result = true;

											}

										},
										type : 'POST',
										async: false
									});

								}

								return result && emailrepeatcheck();

							}

							function emailrepeatcheck() {

								var form = document.getElementById("kayitformu");

								var email = form.elements['user_email'].value.toLowerCase();

								var email2 = form.elements['user_email_confirmation'].value.toLowerCase();

								var check = email == email2;
								
								var result = false;

								if (!check) {

									$(".emailkarakterhatasirepeat").show();

								} else {

									$(".emailkarakterhatasirepeat").hide();
									result = true;

								}
								
								return result;

							}

							
						</script>

						<!--  regexp kodu -->

						<script>
							function vsubmit() {

								var usernameCheck = usernamevalidation();

								var passwordCheck = passwordcheck();

								var nameCheck = namevalidator();

								var surnameCheck = surnamevalidator();

								var emailCheck = emailcheck();

								if (usernameCheck && passwordCheck && nameCheck 
									&& surnameCheck && emailCheck && grecaptcha.getResponse()!="") {

									return true;

								} else {

									return false;

								}

							}
						</script>

					</div>

				</div>

			</div>

