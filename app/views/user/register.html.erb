								<div class="row">
									
									<div style="overflow:hidden" class="g-recaptcha" data-sitekey="6Ld3uhYTAAAAAICn8yQ17ES81jCf7J4Dpz0yUgkf"></div>
									
									<br>
							
									<%= t.submit 'Kayıt Ol' , :class => 'hidden-lg hidden-md col-lg-2  btn btn-primary' , :style => 'margin-left: 20px;' %>
						
									<%= t.submit 'Kayıt Ol' , :class => 'hidden-sm hidden-xs col-lg-2 col-md-2 btn btn-primary'  %>
									
									
