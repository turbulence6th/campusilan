		<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js"></script>

		<script>
			var $tooltip = jQuery.noConflict();
		</script>

		<script type="text/javascript" src="/resources/plugins/tooltipplugin/tooltip.js"></script>

		<!--  user panelinin js kodları -->

		<script src="http://code.jquery.com/jquery-1.11.1.min.js"></script>

		<script
		src="http://netdna.bootstrapcdn.com/bootstrap/3.1.0/js/bootstrap.min.js"></script>

		<script>
			var $uyepaneljs = jQuery.noConflict();
		</script>

		<script type="text/javascript">
			$uyepaneljs(function() {
				var panels = $uyepaneljs('.user-infos');
				var panelsButton = $uyepaneljs('.dropdown-user');
				panels.hide();

				//Click dropdown
				panelsButton.click(function() {
					//get data-for attribute
					var dataFor = $uyepaneljs(this).attr('data-for');
					var idFor = $uyepaneljs(dataFor);

					//current button
					var currentButton = $(this);
					idFor.slideToggle(400, function() {
						//Completed slidetoggle
						if (idFor.is(':visible')) {
							currentButton.html('<i class="glyphicon glyphicon-chevron-up text-muted"></i>');
						} else {
							currentButton.html('<i class="glyphicon glyphicon-chevron-down text-muted"></i>');
						}
					})
				});

				$uyepaneljs('[data-toggle="tooltip"]').tooltip();

				$uyepaneljs('button').click(function(e) {
					e.preventDefault();

				});
			});
		</script>

		<script>
			$(".birincimenu > ul").slideDown(0);
			$(".birincimenu").addClass("active");

		</script>

